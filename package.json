{
  "name": "conditional-stream",
  "version": "1.0.0",
  "description": "A duplex stream that only passes data through when a set function returns true",
  "main": "index.js",
  "scripts": {
    "test": "npm run-script lint && node_modules/.bin/tape test/test.js",
    "lint": "node_modules/.bin/jscs --esnext --config .jscsrc src/*.js && node_modules/.bin/jshint --config .jshintrc src/*.js",
    "build": "babel src/index.js -o ./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/swang/conditional-stream.git"
  },
  "keywords": [
    "streams2",
    "conditional-stream",
    "stream"
  ],
  "devDependencies": {
    "tape": "4.0.x",
    "jshint": "^2.0.0",
    "jscs": "^1.12.0"
  },
  "bugs": {
    "url" : "http://github.com/swang/conditiona-stream/issues"
  },
  "author": "Shuan Wang",
  "license": "MIT"
}
